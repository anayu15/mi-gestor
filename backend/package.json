    "migrate:modelo036-types": "ts-node src/scripts/runMigration034.ts",
    "migrate:reorder-clients": "ts-node src/scripts/runMigration006.ts",
    "migrate:create": "node -r ts-node/register migrations/create.ts",
    "validate:invoices": "ts-node src/scripts/validateInvoiceIntegrity.ts",
    "seed": "ts-node database/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "fiscal",
    "autonomo",
    "trade",
    "tax",
    "spain"
  ],
  "author": "",
  "license": "MIT",
