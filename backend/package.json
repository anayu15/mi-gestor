  "description": "Backend API para miGestor - Gestor fiscal para aut√≥nomos TRADE",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "node -r ts-node/register migrations/run.ts",
    "migrate:preferences": "ts-node src/scripts/runMigration.ts",
    "migrate:modelo115": "ts-node src/scripts/addModelo115AndTarifaPlana.ts",
    "migrate:recurring": "ts-node src/scripts/addRecurringInvoices.ts",
    "migrate:ocr": "ts-node src/scripts/runOCRMigration.ts",
    "migrate:invoice-constraint": "ts-node src/scripts/runMigration013.ts",
    "migrate:rename-expenses": "ts-node src/scripts/runMigration014.ts",
