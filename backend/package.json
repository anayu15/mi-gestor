    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "playwright": "^1.57.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
